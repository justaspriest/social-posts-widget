{
    "compilerOptions": {
        "outDir": "./build/",
        "sourceMap": true,
        "noImplicitAny": true,
        "module": "es6",
        "moduleResolution": "node",
        "target": "es6",
        "jsx": "react",
        "allowSyntheticDefaultImports": true,
        "strictNullChecks": true,
        "strictPropertyInitialization": true
    },
    "include": [
        "./src"
    ]
}